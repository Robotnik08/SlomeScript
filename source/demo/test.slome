>> This is the first script ever written;
DO SAY "Hello\sWorld\sThis\sIs\sSlome\sSpeaking!\n";
MAKE INT coolNumber -12;
DO SAY coolNumber;
DO SAY "\n";
MAKE DOUBLE coolDouble 12.102;
DO SAY coolDouble;
DO SAY "\n";
MAKE STRING Name "Slome\sEpic";
DO SAY Name;
DO SAY "\n";
MAKE BOOL isCool true;
MAKE BOOL isEpic true;
DO SAY isCool;
DO SAY "\n";
DO SAY "Where\sAre\sYou!\n";
DO SAY @isCool==isEpic&&coolNumber!=coolNumber||coolDouble>coolDouble@;
DO SAY "\n\n\n";
SKIP TO "else" UNLESS @coolNumber==coolNumber@;
    DO SAY "They\sare\sthe\ssame!\n";
    SKIP TO "endIf";
SKIP LOCATION else;
    DO SAY "They\sare\snot\sthe\ssame!\n";
SKIP LOCATION endIf;
MAKE DOUBLE i 200;
MAKE DOUBLE loopEndTreshHold 0.8;
SKIP LOCATION loop;
DO SAY i;
DO SAY "\n";
DO SET i :i*0.5:;
SKIP TO "loop" WHEN @i>loopEndTreshHold@;
MAKE DOUBLE j 200;
SKIP LOCATION loops;
DO SAY j;
DO SAY "\n";
DO SET j :j*0.5:;
SKIP TO "loops" WHEN @j>loopEndTreshHold@;