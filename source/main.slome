DO SAY "Jello\n";
MAKE STRING name "Robotnik";
MAKE ARRAY hello 2330,"dwqedwe",213123,220,true,false,true,false,name,true,true,31293.4234;
DO SAY hello,"\n",hello/11,"\n";
MAKE NUMBER num 0;
DO SET num hello/11;
DO SET num :num+12+12+12:;
DO SAY num,"\n";
DO "arrayOfSize" :num:;
END 0;
DEFINE FUNC VOID arrayOfSize :size:;
    MAKE ARRAY res 0;
    SKIP LOCATION loop;
        DO SET res/0 size;
    SKIP TO "loop" WHEN @size>1@;
    DO SAY res,"\n";
    DO RETURN;
ENDFUNC;